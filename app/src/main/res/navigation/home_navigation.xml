<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.quizapp.HomeFragment"
        android:label="home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.example.quizapp.SearchFragment"
        android:label="find"
        tools:layout="@layout/fragment_search" >
    </fragment>

    <fragment
        android:id="@+id/folderFragment"
        android:name="com.example.quizapp.FolderFragment"
        android:label="tv"
        tools:layout="@layout/fragment_folder" >
    </fragment>

    <fragment
        android:id="@+id/personalFragment"
        android:name="com.example.quizapp.PersonalFragment"
        android:label="Cn"
        tools:layout="@layout/fragment_personal" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.quizapp.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/questionFragment"
        android:name="com.example.quizapp.QuestionFragment"
        android:label="QuestionFragment" >

        <action
            android:id="@+id/action_questionFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>